{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/api/index.ts",
    "src/api/world.ts",
    "src/api/entities.ts",
    "src/api/effects.ts",
    "src/api/experience.ts",
    "src/api/systems.ts",
    "src/api/rendering.ts"
  ],
  "entryPointStrategy": "expand",
  "out": "packages/docs",
  "name": "Strata",
  "readme": "docs/GETTING_STARTED.md",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Overview",
    "World Building",
    "Entities & Simulation",
    "Effects & Atmosphere",
    "Player Experience",
    "Game Systems",
    "Rendering Pipeline",
    "*"
  ],
  "navigationLinks": {
    "GitHub": "https://github.com/jbcom/strata",
    "Examples": "/examples/showcase/"
  },
  "customCss": "packages/docs-theme.css",
  "favicon": "packages/docs-favicon.svg",
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}

{
    "name": "@jbcom/strata",
    "version": "1.0.0",
    "description": "Procedural 3D graphics library for React Three Fiber - terrain, water, vegetation, sky, volumetrics, and characters",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./core": {
            "types": "./dist/core/index.d.ts",
            "import": "./dist/core/index.js"
        },
        "./components": {
            "types": "./dist/components/index.d.ts",
            "import": "./dist/components/index.js"
        },
        "./shaders": {
            "types": "./dist/shaders/index.d.ts",
            "import": "./dist/shaders/index.js"
        },
        "./utils": {
            "types": "./dist/utils/index.d.ts",
            "import": "./dist/utils/index.js"
        },
        "./presets": {
            "types": "./dist/presets/index.d.ts",
            "import": "./dist/presets/index.js"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "clean": "rimraf dist packages/docs",
        "docs:dev": "pnpm docs:build && npx serve packages/docs",
        "docs:build": "typedoc",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "format": "prettier --write src/ tests/",
        "format:check": "prettier --check src/ tests/",
        "test": "vitest run",
        "test:unit": "vitest run --config tests/unit/vitest.config.ts",
        "test:integration": "vitest run --config tests/integration/vitest.config.ts",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
        "prepublishOnly": "npm run build && npm run test:unit",
        "prepare": "husky"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{js,json,md,yml,yaml}": [
            "prettier --write"
        ]
    },
    "keywords": [
        "strata",
        "3d-graphics",
        "procedural-generation",
        "three.js",
        "react-three-fiber",
        "r3f",
        "webgl",
        "sdf",
        "marching-cubes",
        "gpu-instancing",
        "raymarching",
        "volumetric",
        "terrain",
        "water",
        "fog",
        "vegetation",
        "fur",
        "character-animation",
        "shaders",
        "glsl"
    ],
    "author": "Jon Bogaty <jon@jonbogaty.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/jbcom/strata"
    },
    "homepage": "https://github.com/jbcom/strata#readme",
    "bugs": {
        "url": "https://github.com/jbcom/strata/issues"
    },
    "peerDependencies": {
        "@react-three/drei": ">=9.0.0",
        "@react-three/fiber": ">=8.0.0",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "three": ">=0.150.0"
    },
    "devDependencies": {
        "@playwright/test": "1.57.0",
        "@react-three/drei": "10.7.7",
        "@react-three/fiber": "9.4.2",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/git": "10.0.1",
        "@testing-library/react": "16.3.0",
        "@types/react": "19.2.7",
        "@types/react-dom": "19.2.3",
        "@types/three": "0.182.0",
        "@typescript-eslint/eslint-plugin": "8.49.0",
        "@typescript-eslint/parser": "8.49.0",
        "@vitest/coverage-v8": "4.0.15",
        "eslint": "9.39.1",
        "husky": "9.1.7",
        "jsdom": "27.3.0",
        "lint-staged": "16.2.7",
        "prettier": "3.7.4",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "rimraf": "6.1.2",
        "semantic-release": "25.0.2",
        "three": "0.182.0",
        "typedoc": "0.28.15",
        "typescript": "5.9.3",
        "vitest": "4.0.15"
    },
    "dependencies": {
        "@dimforge/rapier3d-compat": "0.19.3",
        "@react-three/postprocessing": "3.0.4",
        "@react-three/rapier": "2.2.0",
        "@types/howler": "2.2.12",
        "@xstate/react": "6.0.0",
        "howler": "2.2.4",
        "immer": "11.0.1",
        "lamina": "1.2.2",
        "leva": "0.10.1",
        "maath": "0.10.8",
        "miniplex": "2.0.0",
        "miniplex-react": "2.0.1",
        "ngraph.graph": "20.1.1",
        "ngraph.path": "1.6.1",
        "postprocessing": "6.38.0",
        "serve": "14.2.5",
        "simplex-noise": "4.0.3",
        "tunnel-rat": "0.1.2",
        "xstate": "5.25.0",
        "yuka": "0.7.8",
        "zundo": "2.3.0",
        "zustand": "5.0.9"
    },
    "peerDependenciesMeta": {
        "@use-gesture/react": {
            "optional": true
        },
        "@react-three/xr": {
            "optional": true
        }
    }
}
